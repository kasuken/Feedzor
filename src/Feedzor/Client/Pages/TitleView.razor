@foreach (var item in Items)
{
    <div class="row">
        <div class="col-sm-12">
            <hr />
        </div>
    </div>
    <div class="row">
        <div class="col-sm-11">
            <small><b>@item.Title</b> 
                @((MarkupString)item.Description)
            </small>
        </div>
        <div class="col-sm-1">
            <small>@item.Published.ToString("dd MMM")</small>
        </div>
    </div>
}

@code {

    [Parameter]
    public List<Feedzor.Shared.FeedItem> Items { get; set; }

}
